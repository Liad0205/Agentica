# LLM API Keys
XAI_API_KEY=xai-...
GEMINI_API_KEY=AIzaSy...

# Model Configuration (must include provider prefix for LiteLLM)
DEFAULT_MODEL=gemini/gemini-3-flash-preview
ORCHESTRATOR_MODEL=xai/grok-4-1-fast-reasoning
EVALUATOR_MODEL=xai/grok-4-1-fast-reasoning
USE_MOCK_LLM=false

# Agent Limits
MAX_REACT_ITERATIONS=15
MAX_SUBTASK_ITERATIONS=8
NUM_HYPOTHESIS_AGENTS=3
TOOL_TIMEOUT_SECONDS=60
AGENT_TIMEOUT_SECONDS=300
LLM_RETRY_ATTEMPTS=2

# Sandbox
SANDBOX_IMAGE=agentica-sandbox:latest
SANDBOX_BASE_PORT=5173
MAX_CONCURRENT_SANDBOXES=10
SANDBOX_CLEANUP_INTERVAL_MINUTES=5
SANDBOX_TTL_MINUTES=30
SANDBOX_ALLOW_UNRESTRICTED_COMMANDS=true

# Session
SESSION_TTL_MINUTES=60

# Server
BACKEND_PORT=8000
FRONTEND_PORT=3000
CORS_ORIGINS=http://localhost:3000
LOG_LEVEL=INFO
LOG_FORMAT=json
